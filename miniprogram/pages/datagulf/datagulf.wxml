<!--pages/datagulf/datagulf.wxml-->
<swiper class='cloud-swiper' style='background: #eee' indicator-dots='{{false}}'
   previous-margin="20rpx" next-margin="20rpx"
   current='{{currentIndex}}'>
   <swiper-item wx:for="{{clouds}}"><van-image
      width="24rem"
      height="16rem"
      fit="contain"
      src="{{item}}"
    /></swiper-item>
</swiper>
